{
  "name": "cypress-serverest-automation",
  "version": "1.0.0",
  "description": "Testes automatizados E2E e de API para ServeRest usando Cypress",
  "main": "index.js",
  "scripts": {
    "test": "set ELECTRON_RUN_AS_NODE=&& cypress run",
    "cypress:open": "set ELECTRON_RUN_AS_NODE=&& cypress open",
    "cypress:run": "set ELECTRON_RUN_AS_NODE=&& cypress run",
    "test:e2e": "set ELECTRON_RUN_AS_NODE=&& cypress run --spec \"cypress/e2e/frontend/**/*.cy.js\"",
    "test:api": "set ELECTRON_RUN_AS_NODE=&& cypress run --spec \"cypress/e2e/api/**/*.cy.js\"",
    "cy:run:frontend": "npm run test:e2e",
    "cy:run:api": "npm run test:api",
    "cy:run:all": "npm test",
    "cy:headless": "npm test"
  },
  "keywords": ["cypress", "automation", "testing", "e2e", "api"],
  "author": "QA Automation",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^14.5.4"
  }
}
